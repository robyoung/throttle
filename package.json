{
  "name": "throttle-action",
  "version": "1.0.2",
  "private": true,
  "description": "A GitHub action for serializing jobs",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package-main": "ncc build lib/main.js --out dist/main --source-map --license licenses.txt",
    "package-post": "ncc build lib/post.js --out dist/post --source-map --license licenses.txt",
    "package": "npm run package-main && npm run package-post",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robyoung/throttle.git"
  },
  "keywords": [
    "actions",
    "node",
    "synchronization",
    "serialization",
    "gcs"
  ],
  "author": "Rob Young <rob@robyoung.digital>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@google-cloud/storage": "^5.16.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@typescript-eslint/parser": "^5.8.0",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.5.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.4.5",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "y18n": ">=4.0.1"
  }
}
